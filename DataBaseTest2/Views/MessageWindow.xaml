<Window x:Class="DataBase1WPF.Views.MessageWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DataBase1WPF.Views" xmlns:viewmodels="clr-namespace:DataBase1WPF.ViewModels"
        mc:Ignorable="d"
        Title="MessageWindow" Height="150" Width="300" 
        WindowStartupLocation="CenterScreen" ResizeMode="NoResize">

    <Window.DataContext>
        <viewmodels:SQLqueryVM/>
    </Window.DataContext>

    <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
        <TextBlock HorizontalAlignment="Center" FontSize="20" Height="30"/>
    </StackPanel>
</Window>
